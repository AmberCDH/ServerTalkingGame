<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.text.Text?>
<?import javafx.scene.text.Font?>
<AnchorPane id="HelloApp" fx:controller="com.example.server.HelloController" xmlns:fx="lib" prefHeight="396" prefWidth="478">
    <children>
        <Button fx:id="button_send" layoutX="404" layoutY="338" text="send"/>
        <TextField fx:id="tf_msg" layoutX="30" layoutY="338" prefWidth="361" prefHeight="26"/>
        <ScrollPane fx:id="sp_main" layoutX="30" layoutY="70" prefHeight="256"
        prefWidth="418" fitToWidth="true">
            <content>
                <VBox fx:id="vbox_msg" prefHeight="248" prefWidth="412"/>
            </content>
        </ScrollPane>
        <Label layoutX="114" layoutY="26" text="Amber's messenger">
            <font>
                <Font name="Arial Rounded MT Bold" size="23"></Font>
            </font>
        </Label>
    </children>
</AnchorPane>
